{
  "name": "@uiw/react-amap",
  "version": "1.8.8",
  "description": "基于 React 封装的高德地图组件。AMap Component Based On React.",
  "homepage": "https://uiwjs.github.io/react-amap",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "scripts": {
    "prepare": "husky install && npm run build",
    "doc": "kkt build --app-src ./website",
    "start": "kkt start --app-src ./website",
    "build": "tsbb build",
    "watch": "tsbb watch",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uiwjs/react-amap.git"
  },
  "author": "Kenny Wong <wowohoo@qq.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "files": [
    "src",
    "lib"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx,less,md,json}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "react-amap",
    "amap",
    "map",
    "react",
    "bdmap",
    "高德",
    "地图",
    "高德地图"
  ],
  "dependencies": {
    "@babel/runtime": "7.14.8"
  },
  "devDependencies": {
    "@kkt/less-modules": "6.11.0",
    "@kkt/raw-modules": "6.11.0",
    "@kkt/scope-plugin-options": "6.11.0",
    "@loadable/component": "5.15.0",
    "@types/loadable__component": "5.13.4",
    "@types/react": "17.0.19",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "5.1.8",
    "@uiw/react-code-preview": "1.11.13",
    "@uiw/react-github-corners": "1.4.0",
    "@uiw/react-markdown-preview": "3.2.2",
    "@uiw/reset.css": "1.0.5",
    "cross-env": "7.0.3",
    "husky": "7.0.1",
    "kkt": "6.11.0",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "rehype-attr": "1.4.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0",
    "tsbb": "3.0.0-rc.6"
  },
  "eslintConfig": {
